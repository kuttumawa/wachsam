<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="ehcache.xsd" 
	updateCheck="true"
	monitoring="autodetect" 
	dynamicConfig="true">

	<diskStore path="java.io.tmpdir" />
	
	<cache name="ioCache_default" 
		maxEntriesLocalHeap="10000"
		maxEntriesLocalDisk="1000" 
		eternal="false" 
		diskSpoolBufferSizeMB="20"
		timeToIdleSeconds="300" timeToLiveSeconds="600"
		memoryStoreEvictionPolicy="LFU" 
		transactionalMode="off">
		<persistence strategy="localTempSwap" />
	</cache>
    <cache name="ioCache" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
    <cache name="ioCacheAlertas" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
    <cache name="ioCacheLugar" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
    <cache name="ioCachePeligro" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
    <cache name="ioCacheSitio" maxElementsInMemory="10000" eternal="true" overflowToDisk="false" />
</ehcache>