<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    
 <context-param>
     <param-name>contextConfigLocation</param-name>
     <param-value>classpath:*Context.xml</param-value>
 </context-param>
 <listener>
     <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
</listener>


  <servlet>
  	<servlet-name>Magno</servlet-name>
  	<display-name>Magno</display-name>
  	<description></description>
  	<servlet-class>es.io.wachsam.servlets.Magno</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ProvisionalAlertUpdaterForYou</servlet-name>
  	<display-name>ProvisionalAlertUpdaterForYou</display-name>
  	<description></description>
  	<servlet-class>es.io.wachsam.servlets.ProvisionalAlertUpdaterForYou</servlet-class>
  </servlet>
   <servlet>
  	<servlet-name>ProvisionalPeligroUpdaterForYou</servlet-name>
  	<display-name>ProvisionalPeligroUpdaterForYou</display-name>
  	<description></description>
  	<servlet-class>es.io.wachsam.servlets.ProvisionalPeligroUpdaterForYou</servlet-class>
  </servlet>
    <servlet>
  	<servlet-name>ProvisionalLugarUpdaterForYou</servlet-name>
  	<display-name>ProvisionalLugarUpdaterForYou</display-name>
  	<description></description>
  	<servlet-class>es.io.wachsam.servlets.ProvisionalLugarUpdaterForYou</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>Magno</servlet-name>
  	<url-pattern>/Magno</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProvisionalAlertUpdaterForYou</servlet-name>
  	<url-pattern>/ProvisionalAlertUpdaterForYou</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ProvisionalPeligroUpdaterForYou</servlet-name>
  	<url-pattern>/ProvisionalPeligroUpdaterForYou</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
  	<servlet-name>ProvisionalLugarUpdaterForYou</servlet-name>
  	<url-pattern>/ProvisionalLugarUpdaterForYou</url-pattern>
  </servlet-mapping>

</web-app>
